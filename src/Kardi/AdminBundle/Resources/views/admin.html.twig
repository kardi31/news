<!DOCTYPE html>
<!--[if IE 8]>
<html lang="en" class="ie8 no-js"> <![endif]-->
<!--[if IE 9]>
<html lang="en" class="ie9 no-js"> <![endif]-->
<!--[if !IE]><!-->
<html lang="en">
<!--<![endif]-->
<!-- BEGIN HEAD -->

<head>
    <meta charset="utf-8"/>
    <title>KM CMS - Panel administracyjny</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1" name="viewport"/>

    {% block styles %}
        <!-- BEGIN LAYOUT FIRST STYLES -->
        <link href="//fonts.googleapis.com/css?family=Oswald:400,300,700" rel="stylesheet" type="text/css"/>
        <!-- END LAYOUT FIRST STYLES -->
        <!-- BEGIN GLOBAL MANDATORY STYLES -->
        <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700&subset=all" rel="stylesheet"
              type="text/css"/>
        <link href="{{ asset('bundles/kardiadmin/assets/global/plugins/font-awesome/css/font-awesome.min.css') }}"
              rel="stylesheet" type="text/css"/>
        <link href="{{ asset('bundles/kardiadmin/assets/global/plugins/simple-line-icons/simple-line-icons.min.css') }}"
              rel="stylesheet" type="text/css"/>
        <link href="{{ asset('bundles/kardiadmin/assets/global/plugins/bootstrap/css/bootstrap.min.css') }}"
              rel="stylesheet" type="text/css"/>
        <link href="{{ asset('bundles/kardiadmin/assets/global/plugins/bootstrap-switch/css/bootstrap-switch.min.css') }}"
              rel="stylesheet" type="text/css"/>
        <!-- END GLOBAL MANDATORY STYLES -->
        <!-- BEGIN THEME GLOBAL STYLES -->
        <link href="{{ asset('bundles/kardiadmin/assets/global/css/components.min.css') }}" rel="stylesheet"
              id="style_components" type="text/css"/>
        <link href="{{ asset('bundles/kardiadmin/assets/global/css/plugins.min.css') }}" rel="stylesheet"
              type="text/css"/>
        <!-- END THEME GLOBAL STYLES -->
        <!-- BEGIN THEME LAYOUT STYLES -->
        <link href="{{ asset('bundles/kardiadmin/assets/layouts/layout5/css/layout.min.css') }}" rel="stylesheet"
              type="text/css"/>
        <link href="{{ asset('bundles/kardiadmin/assets/layouts/layout5/css/custom.min.css') }}" rel="stylesheet"
              type="text/css"/>
        <!-- END THEME LAYOUT STYLES -->

        <!-- BEGIN CUSTOM LAYOUT STYLES -->

        {% stylesheets filter="scssphp" output="css/custom.css"
        "@KardiAdminBundle/Resources/Public/custom.scss" %}
        <link rel="stylesheet" href="{{ asset_url }}"/>
        {% endstylesheets %}
        <!-- END CUSTOM LAYOUT STYLES -->
    {% endblock styles %}
    <link rel="shortcut icon" href="favicon.ico"/>


    {% block top_javascripts %}
        <script src="{{ asset('bundles/kardiadmin/assets/global/plugins/jquery.min.js') }}"></script>
    {% endblock top_javascripts %}
</head>
<!-- END HEAD -->

<body class="page-header-fixed page-sidebar-closed-hide-logo">
<!-- BEGIN CONTAINER -->
<div class="wrapper">
    <!-- BEGIN HEADER -->
    {{ include('KardiAdminBundle::navigation.html.twig') }}
    {#{{ include('KardiAdminBundle::default_navigation.html.twig') }}#}
    <!-- END HEADER -->

    <div class="container-fluid">
        <div class="page-content">
            <div id="admin_message_box"></div>

            {% block content %}

            {% endblock content %}
        </div>
        <!-- BEGIN FOOTER -->
        <p class="copyright">
            &copy;  <a href="http://kardimobile.pl">Kardimobile</a>
        </p>
        <a href="#index" class="go2top">
            <i class="icon-arrow-up"></i>
        </a>
        <!-- END FOOTER -->
    </div>
</div>
<!-- END CONTAINER -->

{% block footer_javascripts %}
    <!--[if lt IE 9]>
    <script src="{{ asset('bundles/kardiadmin/assets/global/plugins/respond.min.js')}}"></script>
    <script src="{{ asset('bundles/kardiadmin/assets/global/plugins/excanvas.min.js')}}"></script>
    <script src="{{ asset('bundles/kardiadmin/assets/global/plugins/ie8.fix.min.js')}}"></script>
    <![endif]-->
    <!-- BEGIN CORE PLUGINS -->
    {% javascripts
    '@KardiAdminBundle/Resources/Public/assets/global/plugins/jquery.min.js'
    '@KardiAdminBundle/Resources/Public/assets/global/plugins/bootstrap/js/bootstrap.min.js'
    '@KardiAdminBundle/Resources/Public/assets/global/plugins/js.cookie.min.js'
    '@KardiAdminBundle/Resources/Public/assets/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js'
    '@KardiAdminBundle/Resources/Public/assets/global/plugins/jquery.blockui.min.js'
    '@KardiAdminBundle/Resources/Public/assets/global/plugins/bootstrap-switch/js/bootstrap-switch.min.js'
    output='js/compiled/admin_core_plugins.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', { callback: 'fos.Router.setData' }) }}"></script>

    <script src="{{ asset('bundles/kardiadmin/assets/global/plugins/morris/morris.min.js') }}"
            type="text/javascript"></script>


    <!-- BEGIN THEME GLOBAL SCRIPTS -->
    <!-- BEGIN THEME LAYOUT SCRIPTS -->
    {% javascripts
    '@KardiAdminBundle/Resources/Public/assets/global/scripts/app.min.js'
    '@KardiAdminBundle/Resources/Public/assets/layouts/layout5/scripts/layout.min.js'
    '@KardiAdminBundle/Resources/Public/assets/layouts/global/scripts/quick-sidebar.min.js'
    '@KardiAdminBundle/Resources/Public/assets/layouts/global/scripts/quick-nav.min.js'
    '@KardiAdminBundle/Resources/Public/admin.js'
    output='js/compiled/admin_theme_scripts.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    <!-- END PAGE LEVEL PLUGINS -->
    <!-- END THEME GLOBAL SCRIPTS -->
{% endblock %}
<!-- END THEME LAYOUT SCRIPTS -->
<script>
    $(document).ready(function () {
        $('#clickmewow').click(function () {
            $('#radio1003').attr('checked', 'checked');
        });
    })
</script>
</body>

</html>
